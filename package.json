{
  "name": "vite-plugin-swagger-typescript-api",
  "version": "1.0.1",
  "private": false,
  "description": "基于 swagger-typescript-api 的 vite 插件，自动化生成 API 接口",
  "author": "CaoMeiYouRen",
  "license": "MIT",
  "homepage": "https://github.com/CaoMeiYouRen/vite-plugin-swagger-typescript-api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/vite-plugin-swagger-typescript-api.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/vite-plugin-swagger-typescript-api/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "release": "semantic-release",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production rollup -c",
    "postbuild": "rimraf \"dist/**/!(*.js?(.map)|index.d.ts)\"",
    "analyzer": "cross-env NODE_ENV=production ANALYZER=true rollup -c",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "dev:rollup": "cross-env NODE_ENV=development rollup -c -w",
    "rm": "rimraf node_modules",
    "start": "node ./dist/index",
    "commit": "cz",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "swagger-typescript-api": ">=9",
    "vite": ">=2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-cmyr-config": "^2.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-cmyr": "^1.1.1",
    "debug": "^4.3.7",
    "eslint": "^9.14.0",
    "eslint-config-cmyr": "^1.1.32",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.10",
    "rollup": "^4.24.4",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^24.2.0",
    "swagger-typescript-api": "^13.0.22",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "validate-commit-msg": "^2.14.0",
    "vite": "^5.4.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-cmyr"
    }
  },
  "changelog": {
    "language": "zh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
